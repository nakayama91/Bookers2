<main>
  <div class="container px-5 px-sm-0">
    <div class="row">
      <div class="col-md-8 offset-md-1">

        <% if @model == 'user' %>
          <h2>Users search for "<%= @content %>"</h2>

          <%= render 'users/usertable', users:@records %>

        <% else @model == 'books' %>
          <h2>Books search for "<%= @content %>"</h2>

          <%= render 'books/booktable', books:@records %>

        <% end %>

      </div>
    </div>
  </div>
</main>